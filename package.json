  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/"
  },
  "homepage": "https://plexpy.com",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "tailwindcss",
    "template",
    "radix",
    "react",
    "nextjs"
  ]
